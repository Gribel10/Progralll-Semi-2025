        "import pandas as pd\n",
        "\n",
        "def calificar_y_ordenar_estudiantes(datos_estudiantes):\n",
        "\n",
        "\n",
        "    df = pd.DataFrame(datos_estudiantes)\n",
        "\n",
        "    df['Estado'] = df['Nota'].apply(lambda x: 'Aprobado' if x >= 70 else 'Reprobado')\n",
        "\n",
        "    df_ordenado = df.sort_values(by='Nota', ascending=False)\n",
        "\n",
        "    return df_ordenado\n",
        "\n",
        "datos_ejemplo = {\n",
        "    'Nombre': ['Emilio Chevez', 'Alexia Gomez', 'Marta Rivas', 'Caro Ruiz', 'Elena Soto'],\n",
        "    'Nota': [85, 62, 91, 75, 58]\n",
        "}\n",
        "\n",
        "resultados = calificar_y_ordenar_estudiantes(datos_ejemplo)\n",
        "\n",
        "print(\" Resultados de Calificación y Ordenamiento de Estudiantes:\")\n",
        "print(\"\")\n",
        "print(resultados)"
